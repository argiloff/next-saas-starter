# Basis-URL der Anwendung
NEXT_PUBLIC_BASE_URL=https://sass.optimizemefy.eu
# Postgres-Datenbankverbindung
DATABASE_URL=postgresql://<DB_USER>:<DB_PASSWORD>@<DB_HOST>:5432/<DB_NAME>

# Stripe-Keys (für später, kannst vorerst ignorieren)
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=

# Authentifizierung
AUTH_SECRET=<Ein sicherer Schlüssel, z. B. via openssl generiert>
NEXTAUTH_URL=https://sass.optimizemefy.eu

# Optional: Telemetrie deaktivieren
NEXT_TELEMETRY_DISABLED=1
